{
    "version": 2,
    "builds": [
      {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": { "distDir": "static" }
      },
      {
        "src": "main/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "main/wsgi.py"
      }
    ]
  }